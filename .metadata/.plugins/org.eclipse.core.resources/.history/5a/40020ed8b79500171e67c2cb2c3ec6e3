package mx.edu.uacm.TareaEscuela;

import java.util.Scanner;

public class carroMenu {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Este programa es para usar el carro.");
		carroCoche usar = new carroCoche();
		String sino;
		System.out.println("¿Desea abrir la puerta del coche?");
		Scanner teclado = new Scanner(System.in);
		sino = teclado.next();
		usar.AbrirPuerta(sino);
	}

}
